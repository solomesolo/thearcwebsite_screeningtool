{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/screening/(.*)",
      "dest": "/public/screening/$1"
    },
    {
      "src": "/screening",
      "dest": "/public/screening/index.html"
    }
  ]
}
